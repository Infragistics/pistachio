		<Style x:Key="btnFetchKeys" BasedOn="{x:Null}" TargetType="{x:Type Button}">
			<Setter Property="Template">
				<Setter.Value>
					<ControlTemplate TargetType="{x:Type Button}">
						<Grid>
							<Image x:Name="image" Source="Resources\Images\pistachio_logo.png" Stretch="Uniform"/>
						</Grid>
						<ControlTemplate.Triggers>
							<Trigger Property="IsFocused" Value="True"/>
							<Trigger Property="IsDefaulted" Value="True"/>
							<Trigger Property="IsMouseOver" Value="True">
								<Setter Property="BitmapEffect" TargetName="image">
									<Setter.Value>
										<BlurBitmapEffect Radius="3"/>
									</Setter.Value>
								</Setter>
								<Setter Property="Opacity" TargetName="image" Value="1"/>
							</Trigger>
							<Trigger Property="IsPressed" Value="True"/>
							<Trigger Property="IsEnabled" Value="False"/>
						</ControlTemplate.Triggers>
					</ControlTemplate>
				</Setter.Value>
			</Setter>
