				<Setter.Value>
					<SolidColorBrush/>
				</Setter.Value>
			</Setter>
			<Setter Property="BorderThickness" Value="2,2,2,2"/>
			<Setter Property="Padding" Value="2,0,0,0"/>
			<Setter Property="FocusVisualStyle">
				<Setter.Value>
