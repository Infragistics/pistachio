						<Grid x:Name="outerGrid" Width="{Binding Path=(Infragistics_ToyBox:Reflector.ReflectionTarget).ActualWidth, RelativeSource={RelativeSource TemplatedParent}}" Height="{Binding Path=(Infragistics_ToyBox:Reflector.ReflectionTarget).ActualHeight, RelativeSource={RelativeSource TemplatedParent}}">
